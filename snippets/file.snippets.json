{
    "FileCopy": {
        "prefix": "FileCopy",
        "body": "FileCopy, ${1:Source}, ${2:Dest [}, ${3:Flag (1 = overwrite)]}",
        "description": "Copies one or more files."
    },
    "FileCopyDir": {
        "prefix": "FileCopyDir",
        "body": "FileCopyDir, ${1:Source}, ${2:Dest [}, ${3:Flag]}",
        "description": "Copies a folder along with all its sub-folders and files (similar to xcopy)."
    },
    "FileCreateDir": {
        "prefix": "FileCreateDir",
        "body": "FileCreateDir, ${1:Path}",
        "description": "Creates a directory/folder."
    },
    "FileCreateShortcut": {
        "prefix": "FileCreateShortcut",
        "body": "FileCreateShortcut, ${1:Target}, ${2:C:\\My Shortcut.lnk [}, ${3:WorkingDir}, ${4:Args}, ${5:Description}, ${6:IconFile}, ${7:ShortcutKey}, ${8:IconNumber}",
        "description": "Creates a shortcut (.lnk) file."
    },
    "FileDelete": {
        "prefix": "FileDelete",
        "body": "FileDelete, ${1:FilePattern}",
        "description": "Deletes one or more files."
    },
    "FileEncoding": {
        "prefix": "FileEncoding",
        "body": "FileEncoding, ${1:[Encoding]}",
        "description": "Sets the default encoding for FileRead, FileReadLine, Loop Read, FileAppend, and FileOpen()."
    },

    "FileGetShortcut": {
        "prefix": "FileGetShortcut",
        "body": "FileGetShortcut, ${1:LinkFile [}, ${2:OutTarget}, ${3:OutDir}, ${4:OutArgs}, ${5:OutDescription}, ${6:OutIcon}, ${7:OutIconNum}, ${8:OutRunState]}",
        "description": "Retrieves information about a shortcut (.lnk) file, such as its target file."
    },
    "FileGetSize": {
        "prefix": "FileGetSize",
        "body": "FileGetSize, ${1:OutputVar [}, ${2:Filename}, ${3:Units]}",
        "description": "Retrieves the size of a file."
    },
    "FileGetTime": {
        "prefix": "FileGetTime",
        "body": "FileGetTime, ${1:OutputVar [}, ${2:Filename}, ${3:WhichTime (M}, ${4:C}, ${5:or A -- default is M)]}",
        "description": "Retrieves the datetime stamp of a file or folder."
    },
    "FileGetVersion": {
        "prefix": "FileGetVersion",
        "body": "FileGetVersion, ${1:OutputVar [}, ${2:Filename]}",
        "description": "Retrieves the version of a file."
    },
    "FileInstall": {
        "prefix": "FileInstall",
        "body": "FileInstall, ${1:Source}, ${2:Dest [}, ${3:Flag (1 = overwrite)]}",
        "description": "Includes the specified file inside the compiled version of the script."
    },
    "FileMove": {
        "prefix": "FileMove",
        "body": "FileMove, ${1:Source}, ${2:Dest [}, ${3:Flag (1 = overwrite)]}",
        "description": "Moves or renames one or more files."
    },
    "FileMoveDir": {
        "prefix": "FileMoveDir",
        "body": "FileMoveDir, ${1:Source}, ${2:Dest [}, ${3:Flag (2 = overwrite)]}",
        "description": "Moves a folder along with all its sub-folders and files. It can also rename a folder."
    },

    "FileReadLine": {
        "prefix": "FileReadLine",
        "body": "FileReadLine, ${1:OutputVar}, ${2:Filename}, ${3:LineNum}",
        "description": "Reads the specified line from a file and stores the text in a variable."
    },
    "FileRecycle": {
        "prefix": "FileRecycle",
        "body": "FileRecycle, ${1:FilePattern}",
        "description": "Sends a file or directory to the recycle bin if possible, or permanently deletes it."
    },
    "FileRecycleEmpty": {
        "prefix": "FileRecycleEmpty",
        "body": "FileRecycleEmpty, ${1:[ C:\\]}",
        "description": "Empties the recycle bin."
    },
    "FileRemoveDir": {
        "prefix": "FileRemoveDir",
        "body": "FileRemoveDir, ${1:Path [}, ${2:Recurse? (1 = yes)]}",
        "description": "Deletes a folder."
    },
    "FileSelectFile": {
        "prefix": "FileSelectFile",
        "body": "FileSelectFile, ${1:OutputVar [}, ${2:Options}, ${3:RootDir[\\DefaultFilename]}, ${4:Prompt}, ${5:Filter]}",
        "description": "Displays a standard dialog that allows the user to open or save file(s)."
    },
    "FileSelectFolder": {
        "prefix": "FileSelectFolder",
        "body": "FileSelectFolder, ${1:OutputVar [}, ${2:*StartingFolder}, ${3:Options}, ${4:Prompt]}",
        "description": "Displays a standard dialog that allows the user to select a folder."
    },
    "FileSetAttrib": {
        "prefix": "FileSetAttrib",
        "body": "FileSetAttrib, ${1:Attributes(+-^RASHNOT) [}, ${2:FilePattern}, ${3:OperateOnFolders?}, ${4:Recurse?]}",
        "description": "Changes the attributes of one or more files or folders. Wildcards are supported."
    },
    "FileSetTime": {
        "prefix": "FileSetTime",
        "body": "FileSetTime, ${1:[ YYYYMMDDHH24MISS}, ${2:FilePattern}, ${3:WhichTime (M|C|A)}, ${4:OperateOnFolders?}, ${5:Recurse?]}",
        "description": "Changes the datetime stamp of one or more files or folders. Wildcards are supported."
    }
}
