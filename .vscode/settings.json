{
    "[javascript]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "[json]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "[markdown]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "[rust]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "[toml]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "[typescript]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "cSpell.enableFiletypes": [
        "ahk",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "html",
        "ini",
        "javascript",
        "javascriptreact",
        "julia",
        "juliamarkdown",
        "latex",
        "markdown",
        "python",
        "restructuredtext",
        "scss",
        "text",
        "typescript",
        "typescriptreact"
    ],
    "cSpell.words": [
        "Fira",
        "Gdip",
        "Gdiplus",
        "HWND",
        "Ifmsgbox",
        "LTRB",
        "Listenr",
        "Mult",
        "Neko",
        "Subcommand",
        "YYYYMMDDHH",
        "autohotkey",
        "cpuprofile",
        "cputemp",
        "cweijan",
        "dbgp",
        "debuggin",
        "esbuild",
        "esmodule",
        "fullname",
        "gosub",
        "hotstring",
        "ifnotinstring",
        "msgbox",
        "plusplus",
        "pnpm",
        "readonly",
        "refrence",
        "resmon",
        "rslintrc",
        "struct",
        "tabnine",
        "uint",
        "vsocde"
    ],
    "editor.codeActionsOnSave": {
        "addMissingImports": true,
        "source.fixAll": true,
        "source.organizeImports": true
    },
    "editor.defaultFormatter": "dprint.dprint",
    "eslint.enable": true,
    "eslint.options": {
        "extensions": [
            ".ts"
        ]
    },
    "eslint.packageManager": "pnpm",
    "eslint.probe": [
        "typescript"
    ],
    "files.associations": {
        ".eslintrc.json": "jsonc",
        "dprint.json": "jsonc",
        "extensions.json": "jsonc",
        "settings.json": "jsonc",
        "tsconfig.json": "jsonc"
    }
}
