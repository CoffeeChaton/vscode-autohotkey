{
    "name": "vscode-autohotkey-neko-help",
    "displayName": "AutoHotkey NekoHelp",
    "description": "AutoHotkey IntelliSense, format, linters, and language support for VS Code, ",
    "version": "2.5.0",
    "publisher": "CoffeeChaton",
    "engines": {
        "vscode": "^1.69.1"
    },
    "activationEvents": [
        "onLanguage:ahk",
        "onCommand:ahk.nekoHelp.refreshResource"
    ],
    "author": "Coffee Chaton(https://github.com/CoffeeChaton)",
    "main": "./dict/extension",
    "homepage": "https://github.com/CoffeeChaton/vscode-ahk-outline/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/CoffeeChaton/vscode-ahk-outline/issues"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Linters",
        "Formatters"
    ],
    "license": "GNU GPLV2",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "keywords": [
        "ahk",
        "autohotkey",
        "vscode"
    ],
    "contributes": {
        "commands": [
            {
                "command": "ahk.nekoHelp.bar",
                "title": "ahk:List all ahk-neko-help command"
            },
            {
                "command": "ahk.nekoHelp.refreshResource",
                "title": "Refresh Resource (AHK)"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "AhkNekoHelp",
            "properties": {
                "AhkNekoHelp.open.Documents.Uri": {
                    "type": "string",
                    "default": "https://www.autohotkey.com/docs/AutoHotkey.htm",
                    "description": "the doc uri"
                },
                "AhkNekoHelp.Diag.WarningCap.code502": {
                    "type": "number",
                    "default": 10,
                    "minimum": 0,
                    "maximum": 999,
                    "description": "(code502) case sensitivity check of variable, a file max checks number.\\n use 0 to close warn."
                },
                "AhkNekoHelp.Diag.WarningCap.code503": {
                    "type": "number",
                    "default": 10,
                    "minimum": 0,
                    "maximum": 999,
                    "description": "(code503) case sensitivity check of parameter, a file max checks number.\\n use 0 to close warn."
                },
                "AhkNekoHelp.baseScan.IgnoredList": {
                    "type": "array",
                    "default": [
                        "/\\.",
                        "/node_modules$",
                        "/node_modules/",
                        "/neko_Exp_GDIP\\.ahk$"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "description": "Disable AhkNekoHelp when the file path contains a match of any of the provided RegExpArray. Note: pathname separator use \"/\" not windows style \"\\\""
                },
                "AhkNekoHelp.format.textReplace": {
                    "type": "boolean",
                    "default": false,
                    "description": "test options: textReplace"
                },
                "AhkNekoHelp.lint.funcSize": {
                    "type": "number",
                    "default": 100,
                    "minimum": 3,
                    "maximum": 100000,
                    "description": "lint of function/Method size (line)"
                },
                "AhkNekoHelp.snippets.blockFilesList": {
                    "type": "array",
                    "default": [
                        "/Gdip_all_2020_08_24\\.ahk"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "description": "Disable IntelliSense when the file path contains a match of any of the provided RegExpArray. Note: pathname separator use \"/\" not windows style \"\\\""
                },
                "AhkNekoHelp.statusBar.displayColor": {
                    "type": "string",
                    "default": "",
                    "description": "color of status bar, use HTML Color, exp: '#e783e1' or 'rgb(255,255,100)"
                },
                "AhkNekoHelp.useCodeLens": {
                    "type": "boolean",
                    "default": false,
                    "description": "dev options: has some Bug now."
                }
            }
        },
        "languages": [
            {
                "id": "ahk",
                "aliases": [
                    "AutoHotkey",
                    "autohotkey",
                    "ahk"
                ],
                "extensions": [
                    ".ahk"
                ],
                "configuration": "./config/ahk.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ahk",
                "scopeName": "source.ahk",
                "path": "./syntaxes/ahk.tmLanguage.json"
            },
            {
                "language": "log",
                "scopeName": "source.log",
                "path": "./syntaxes/output.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ahk",
                "path": "./snippets/ahk.snippets.json"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "ahk.nekoHelp.refreshResource",
                    "when": "explorerViewletVisible"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "node esbuild.config.js",
        "fmt": "dprint fmt",
        "lint": "eslint src",
        "auto_fix": "eslint --fix src",
        "compile": "node esbuild.config.js",
        "watch": "tsc -watch -p ./",
        "test_vsc": "pnpm run compile && node ./node_modules/vscode/bin/test",
        "test": "jest",
        "up_dprint_config": "dprint config update",
        "up": "pnpm up -L"
    },
    "devDependencies": {
        "@swc/core": "^1.2.218",
        "@swc/jest": "^0.2.22",
        "@types/assert": "^1.5.6",
        "@types/jest": "^28.1.6",
        "@types/micromatch": "^4.0.2",
        "@types/node": "^18.0.6",
        "@types/vscode": "^1.69.0",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "@vscode/test-electron": "^2.1.5",
        "dprint": "^0.30.3",
        "esbuild": "^0.14.49",
        "esbuild-copy-static-files": "^0.1.0",
        "eslint": "^8.20.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-sukka": "^1.3.0",
        "eslint-import-resolver-typescript": "^3.3.0",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-immutable": "^1.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.0",
        "eslint-plugin-no-setter-return": "^0.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-security": "^1.5.0",
        "eslint-plugin-unicorn": "^43.0.2",
        "fs": "0.0.1-security",
        "jest": "^28.1.3",
        "readonly-types": "^3.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
