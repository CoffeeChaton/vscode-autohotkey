{
    "name": "vscode-autohotkey-neko-help",
    "displayName": "AutoHotkeyNekoHelp",
    "description": "AutoHotkey help of VSCode",
    "version": "2.4.0",
    "publisher": "CoffeeChaton",
    "engines": {
        "vscode": "^1.64.0"
    },
    "activationEvents": [
        "onLanguage:ahk",
        "onDebug"
    ],
    "main": "./dict/extension",
    "homepage": "https://github.com/CoffeeChaton/vscode-ahk-outline/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/CoffeeChaton/vscode-ahk-outline/issues"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "keywords": [
        "vscode",
        "autohotkey",
        "ahk"
    ],
    "contributes": {
        "breakpoints": [
            {
                "language": "ahk"
            }
        ],
        "debuggers": [
            {
                "type": "ahk",
                "label": "AutoHotkey",
                "program": "./out/debugEntrance.js",
                "runtime": "\"C:\\Program Files\\AutoHotkey\\AutoHotkeyU64.exe\"",
                "configurationAttributes": {
                    "launch": {
                        "required": [
                            "program"
                        ],
                        "properties": {
                            "program": {
                                "type": "string",
                                "description": "Absolute path to a text file.",
                                "default": "${file}"
                            },
                            "runtime": {
                                "type": "string",
                                "description": "Absolute path to a AutoHotkey.exe file.",
                                "default": "C:\\Program Files\\AutoHotkey\\AutoHotkeyU64.exe"
                            },
                            "stopOnEntry": {
                                "type": "boolean",
                                "description": "Automatically stop after launch.",
                                "default": true
                            },
                            "trace": {
                                "type": "boolean",
                                "description": "Enable logging of the Debug Adapter Protocol.",
                                "default": true
                            },
                            "dbgpSettings": {
                                "type": "object",
                                "properties": {
                                    "max_children": {
                                        "type": "integer",
                                        "description": "max number of array or object children to initially retrieve",
                                        "default": 300
                                    },
                                    "max_data": {
                                        "type": "integer",
                                        "description": "max amount of variable data to initially retrieve.",
                                        "default": 131072
                                    }
                                },
                                "description": "Dbgp settings. See https://xdebug.org/docs-dbgp.php#feature-names",
                                "default": {}
                            }
                        }
                    }
                },
                "initialConfigurations": [
                    {
                        "type": "ahk",
                        "request": "launch",
                        "name": "AutoHotkey Debugger",
                        "program": "${file}",
                        "stopOnEntry": true
                    }
                ],
                "configurationSnippets": [
                    {
                        "label": "AutoHotkey Debug: Launch",
                        "body": {
                            "type": "ahk",
                            "request": "launch",
                            "name": "AutoHotkey Debugger",
                            "program": "${file}",
                            "stopOnEntry": true
                        }
                    }
                ]
            }
        ],
        "commands": [
            {
                "command": "ahk.bar.click",
                "title": "ahk:List all ahk-neko-help command"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "AhkNekoHelp",
            "properties": {
                "AhkNekoHelp.statusBar.displayColor": {
                    "type": "string",
                    "default": "",
                    "description": "color of status bar, use '' to use theme color,or use HTML Color, exp: '#e783e1' or 'rgb(255,255,100)"
                },
                "AhkNekoHelp.format.textReplace": {
                    "type": "boolean",
                    "default": false,
                    "description": "test options: call textReplace & diff"
                },
                "AhkNekoHelp.lint.funcSize": {
                    "type": "number",
                    "default": 100,
                    "minimum": 3,
                    "maximum": 100000,
                    "description": "lint of function/Method size (line)"
                },
                "AhkNekoHelp.Ignored.folder.startsWith": {
                    "type": "array",
                    "default": [
                        ".git",
                        ".vscode",
                        ".history",
                        "node_modules"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "description": "Do not scan while folder names startsWith this array"
                },
                "AhkNekoHelp.Ignored.folder.endsWith": {
                    "type": "array",
                    "default": [
                        "TEST",
                        "Test"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "description": "Do not scan while folder names endsWith this array"
                },
                "AhkNekoHelp.Ignored.File.startsWith": {
                    "type": "array",
                    "default": [],
                    "items": {
                        "type": "string"
                    },
                    "description": "Do not scan while File names startsWith this array"
                },
                "AhkNekoHelp.Ignored.File.endsWith": {
                    "type": "array",
                    "default": [],
                    "items": {
                        "type": "string"
                    },
                    "description": "Do not scan while File names endsWith this array"
                },
                "AhkNekoHelp.Debug.executePath": {
                    "type": "string",
                    "default": "C:/Program Files/AutoHotkey/AutoHotkeyU64.exe",
                    "description": "Execute path of autohotkey."
                }
            }
        },
        "languages": [
            {
                "id": "ahk",
                "aliases": [
                    "AutoHotkey",
                    "autohotkey",
                    "ahk"
                ],
                "extensions": [
                    ".ahk",
                    ".ext"
                ],
                "configuration": "./config/ahk.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ahk",
                "scopeName": "source.ahk",
                "path": "./syntaxes/ahk.tmLanguage.json"
            },
            {
                "language": "Log",
                "scopeName": "source.log",
                "path": "./syntaxes/output.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ahk",
                "path": "./snippets/ahk.snippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node esbuild.config.js",
        "fmt": "dprint fmt",
        "lint": "eslint --fix src",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "jest",
        "test_vsc": "pnpm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@jest/globals": "^27.5.1",
        "@types/assert": "^1.5.6",
        "@types/benchmark": "^2.1.1",
        "@types/glob": "^7.2.0",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.18",
        "@types/temp": "^0.9.1",
        "@types/vscode": "^1.64.0",
        "@types/xml2js": "^0.4.9",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "@vscode/test-electron": "^2.1.2",
        "dprint": "^0.22.2",
        "esbuild": "^0.14.22",
        "esbuild-copy-static-files": "^0.1.0",
        "eslint": "^8.9.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-sukka": "^1.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-immutable": "^1.0.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-jest-formatting": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-no-setter-return": "^0.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-security": "^1.4.0",
        "fs": "0.0.1-security",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5",
        "vscode-languageserver": "^7.0.0",
        "vscode-languageserver-protocol": "^3.16.0",
        "vscode-languageserver-textdocument": "^1.0.4",
        "vscode-languageserver-types": "^3.16.0",
        "vscode-oniguruma": "^1.6.1"
    },
    "dependencies": {
        "@vscode/debugadapter": "^1.53.0",
        "@vscode/debugprotocol": "^1.53.0",
        "get-port-please": "^2.3.0",
        "temp": "^0.9.4",
        "xml2js": "^0.4.23"
    }
}
